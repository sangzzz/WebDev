<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <label for="title">Title</label>
  <br>
  <input id="title" type="text" name="title" value="" placeholder="Enter Title" autocomplete="off">
  <br>
  <label for="post">Post</label>
  <br>
  <textarea id="post" name="data" rows="8" cols="140" placeholder="Enter Post"></textarea>
  <br>
  <button type="submit" name="submit" class="btn btn-lg btn-success">Publish</button>
</form>

<%-include("partials/footer") -%>
